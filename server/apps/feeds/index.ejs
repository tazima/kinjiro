
<% include ../../views/head %>
<div class="navbar navbar-inverse">
  <div class="navbar-inner">
    <div class="container">
      <form class="navbar-form pull-right" action="/session" method="POST">
        <input class="btn btn-primary" type="submit" value="logout" />
        <input type="hidden" name="_method" value="delete" />
      </form>
    </div>
  </div>
</div>

<section id="content" class="container-fluid">
  <div class="row-fluid">
    <div class="span3 feeds">
      <form class="subscribe" action="#" method="">
        <input type="text" name="url" />
        <input type="submit" value="add" />
      </form>
      <div class="feed-list"></div>
    </div>
    <div class="span9 posts"></div>
  </div>
</section>
<script src="/build.js"></script>
<script>
  (function() {
    var Feeds = require("feed-collection"),
        feeds = new Feeds;
    require("boot")({
      feeds: feeds.reset(<%- JSON.stringify(feeds) %>)
    });
  }());
</script>
<% include ../../views/foot %>
